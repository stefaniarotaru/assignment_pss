<Configuration status="debug" name="default-config" packages="">
    <Appenders>
        <Console name="stdout" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n"/>
        </Console>
    </Appenders>

    <Root level="error">
        <AppenderRef ref="STDOUT"/>
    </Root>
</Configuration>

